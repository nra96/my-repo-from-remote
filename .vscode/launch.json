        "version": "0.2.0",
        "configurations": [

            {
                "name": "Launch Chrome against localhost",
                "type": "chrome",
                "request": "launch",
                "url": "http://localhost:4200",
                "webRoot": "${workspaceRoot}"
            },
            {
                "name": "Attach to Chrome",
                "type": "chrome",
                "request": "attach",
                "port": 4200,
                "webRoot": "${workspaceRoot}"
            },
            {
                "name": "Launch Chrome",
                "request": "launch",
                "type": "pwa-chrome",
                "url": "http://localhost:3000",
                "webRoot": "${workspaceFolder}/public",
                "runtimeExecutable": "C:/Users/HP/AppData/Local/Google/Chrome Dev/User Data/Default"
              }
        ]
    }